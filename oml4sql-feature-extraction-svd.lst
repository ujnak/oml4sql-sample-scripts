SQL> -- Build an SVD model
SQL> DECLARE
  2    v_setlst DBMS_DATA_MINING.SETTING_LIST;
  3  BEGIN
  4    -- Select SVD as the Feature Extraction algorithm
  5    v_setlst('ALGO_NAME') := 'ALGO_SINGULAR_VALUE_DECOMP';
  6    -- Turn on automatic data preparation
  7    v_setlst('PREP_AUTO') := 'OFF';
  8    -- Select PCA mode of scoring
  9    v_setlst('SVDS_SCORING_MODE') := 'SVDS_SCORING_PCA';
 10    v_setlst('PREP_SHIFT_2DNUM')  := 'PREP_SHIFT_MEAN';
 11    v_setlst('PREP_SCALE_2DNUM')  := 'PREP_SCALE_STDDEV';
 12
 13    -- Examples of possible overrides are:
 14    -- v_setlst('FEAT_NUM_FEATURES') := '10';
 15    -- v_setlst('SVDS_U_MATRIX_OUTPUT') := 'SVDS_U_MATRIX_ENABLE';
 16    -- v_setlst('ODMS_APPROXIMATE_COMPUTATION') := 'ODMS_APPR_COMP_ENABLE';
 17
 18    DBMS_DATA_MINING.CREATE_MODEL2(
 19      model_name          => 'SVD_SH_sample',
 20      mining_function     => 'FEATURE_EXTRACTION',
 21      data_query          => 'SELECT * FROM svd_sh_sample_build_num',
 22      set_list            => v_setlst,
 23      case_id_column_name => 'cust_id');
 24  END;
