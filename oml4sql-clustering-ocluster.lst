SQL> -------------------
SQL> -- SPECIFY SETTINGS
SQL> --
SQL> -- K-Means is the default clustering algorithm. Override the
SQL> -- default to set the algorithm to O-Cluster.
SQL> --
SQL> ---------------------
SQL> -- CREATE A NEW MODEL
SQL> --
SQL> -- Build a new OC model
SQL> -- TO_CHAR function is used to transform columns to
SQL> -- categorical attributes since numeric datatypes
SQL> -- are treated as numeric attributes.
SQL> DECLARE
  2    xformlist dbms_data_mining_transform.TRANSFORM_LIST;
  3    v_setlst DBMS_DATA_MINING.SETTING_LIST;
  4  BEGIN
  5    dbms_data_mining_transform.SET_TRANSFORM(
  6      xformlist, 'AFFINITY_CARD', null, 'TO_CHAR(AFFINITY_CARD)', null);
  7    dbms_data_mining_transform.SET_TRANSFORM(
  8      xformlist, 'BOOKKEEPING_APPLICATION', null, 'TO_CHAR(BOOKKEEPING_APPLICATION)', null);
  9    dbms_data_mining_transform.SET_TRANSFORM(
 10      xformlist, 'BULK_PACK_DISKETTES', null, 'TO_CHAR(BULK_PACK_DISKETTES)', null);
 11    dbms_data_mining_transform.SET_TRANSFORM(
 12      xformlist, 'FLAT_PANEL_MONITOR', null, 'TO_CHAR(FLAT_PANEL_MONITOR)', null);
 13    dbms_data_mining_transform.SET_TRANSFORM(
 14      xformlist, 'HOME_THEATER_PACKAGE', null, 'TO_CHAR(HOME_THEATER_PACKAGE)', null);
 15    dbms_data_mining_transform.SET_TRANSFORM(
 16      xformlist, 'OS_DOC_SET_KANJI', null, 'TO_CHAR(OS_DOC_SET_KANJI)', null);
 17    dbms_data_mining_transform.SET_TRANSFORM(
 18      xformlist, 'PRINTER_SUPPLIES', null, 'TO_CHAR(PRINTER_SUPPLIES)', null);
 19    dbms_data_mining_transform.SET_TRANSFORM(
 20      xformlist, 'Y_BOX_GAMES', null, 'TO_CHAR(Y_BOX_GAMES)', null);
 21
 22    v_setlst('ALGO_NAME')         := 'ALGO_O_CLUSTER';
 23    v_setlst('CLUS_NUM_CLUSTERS') := '10';
 24    v_setlst('PREP_AUTO')         := 'ON';
 25    -- Other possible settings are:
 26    -- v_setlst('OCLT_SENSITIVITY') := '0.5';
 27
 28    DBMS_DATA_MINING.CREATE_MODEL2(
 29      model_name          => 'OC_SH_Clus_sample',
 30      mining_function     => 'CLUSTERING',
 31      data_query          => 'select * from mining_data_build_parallel_v',
 32      case_id_column_name => 'cust_id',
 33      set_list            => v_setlst,
 34      xform_list          => xformlist);
 35  END;
 36  /

