SQL> -- CREATE A NEW MODEL
SQL> --
SQL> -- Build NMF model
SQL> DECLARE
  2    v_setlst DBMS_DATA_MINING.SETTING_LIST;
  3  BEGIN
  4    -- NMF is the default Feature Extraction algorithm. For this sample,
  5    -- we use Data Auto Preparation.
  6    v_setlst('PREP_AUTO') := 'ON';
  7
  8    -- Other examples of possible overrides are:
  9    -- v_setlst('FEAT_NUM_FEATURES')   := '10';
 10    -- v_setlst('NMFS_CONV_TOLERANCE') := '0.05';
 11    -- v_setlst('NMFS_NUM_ITERATIONS') := '50';
 12    -- v_setlst('NMFS_RANDOM_SEED')    := '-1';
 13
 14    DBMS_DATA_MINING.CREATE_MODEL2(
 15      model_name          => 'NMF_SH_sample',
 16      mining_function     => 'FEATURE_EXTRACTION',
 17      data_query          => 'SELECT * FROM mining_data_build_v',
 18      set_list            => v_setlst,
 19      case_id_column_name => 'cust_id');
 20  END;
 21  /

