SQL> -- Build a new partitioned SVM model
SQL> DECLARE
  2    v_setlst DBMS_DATA_MINING.SETTING_LIST;
  3  BEGIN
  4    v_setlst('ALGO_NAME')              := 'ALGO_SUPPORT_VECTOR_MACHINES';
  5    v_setlst('PREP_AUTO')              := 'ON';
  6    v_setlst('SVMS_KERNEL_FUNCTION')   := 'SVMS_LINEAR';
  7    v_setlst('ODMS_PARTITION_COLUMNS') := 'CUST_GENDER';
  8
  9    DBMS_DATA_MINING.CREATE_MODEL2(
 10      model_name          => 'part_clas_sample',
 11      mining_function     => 'CLASSIFICATION',
 12      data_query          => 'select * from mining_data_build_parallel_v',
 13      set_list            => v_setlst,
 14      case_id_column_name => 'cust_id',
 15      target_column_name  => 'affinity_card');
 16  END;
 17  /

