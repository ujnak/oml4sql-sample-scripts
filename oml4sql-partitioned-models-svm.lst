SQL> -- Build another partitioned model with two partition columns
SQL> -- with three partition values for CUST_INCOME_LEVEL
SQL> DECLARE
  2    xform_list dbms_data_mining_transform.TRANSFORM_LIST;
  3    v_setlst DBMS_DATA_MINING.SETTING_LIST;
  4  BEGIN
  5    dbms_data_mining_transform.set_transform(xform_list,
  6      'CUST_INCOME_LEVEL', null,
  7      'CASE CUST_INCOME_LEVEL WHEN ''A: Below 30,000'' THEN ''LOW''
  8      WHEN ''L: 300,000 and above'' THEN ''HIGH''
  9      ELSE ''MEDIUM'' END', null);
 10
 11    v_setlst('ALGO_NAME')               := 'ALGO_SUPPORT_VECTOR_MACHINES';
 12    v_setlst('PREP_AUTO')               := 'ON';
 13    v_setlst('SVMS_KERNEL_FUNCTION')    := 'SVMS_LINEAR';
 14    v_setlst('ODMS_PARTITION_COLUMNS')  := 'CUST_GENDER,CUST_INCOME_LEVEL';
 15
 16    DBMS_DATA_MINING.CREATE_MODEL2(
 17      model_name          => 'part2_clas_sample',
 18      mining_function     => 'CLASSIFICATION',
 19      data_query          => 'select * from mining_data_build_parallel_v',
 20      set_list            => v_setlst,
 21      case_id_column_name => 'cust_id',
 22      target_column_name  => 'affinity_card',
 23      xform_list          => xform_list);
 24  END;
 25  /

PL/SQLプロシージャが正常に完了しました。

